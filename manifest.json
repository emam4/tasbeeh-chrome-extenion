{
    "name": "Tasbeeh",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Tasbeeh",
    "action": {
        "default_icon": "seb7a.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "js": ["main.js"],
            "matches": [
                "https://*/*"
            ]
        }
    ],
    "permissions": ["tabs"]

}